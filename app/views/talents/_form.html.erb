<%= simple_form_for(@talent) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.association :skilltype %>
    <%= f.input :video %>
    <%= f.input :bio %>
    <%= f.input :resume %>
    <%= f.input :suburb %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
