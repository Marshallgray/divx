<h1>Listing talent profiles</h1>

<table class="table">
    <tr>
      <th>User</th>
      <th>Video</th>
      <th>Bio</th>
      <th>Resume</th>
      <th>Suburb</th>
      <th>Latitude</th>
      <th>Longitude</th>
        <th colspan="3"></th>
    </tr>

  <%= content_tag_for(:tr, @talents) do |talent| %>
    <td><%= talent.user %></td>
    <td><%= talent.video %></td>
    <td><%= talent.bio %></td>
    <td><%= talent.resume %></td>
    <td><%= talent.suburb %></td>
    <td><%= talent.latitude %></td>
    <td><%= talent.longitude %></td>
      <td><%= link_to 'Show', talent, class: 'btn btn' %></td>
    <td><%= link_to 'Edit', edit_talent_path(talent), class: 'btn btn-edit' %></td>
    <td><%= link_to 'Delete', talent, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-destroy' %></td>
  <% end %>
</table>


<div class="form-actions">
  <%= link_to 'New Talent', new_talent_path, class: 'btn btn-success' %>
</div>

<%= will_paginate @talents %>
