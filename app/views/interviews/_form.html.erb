<%= simple_form_for(@interview) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :job_role %>
    <%= f.association :talent %>
    <%= f.input :time %>
    <%= f.input :date %>
    <%= f.input :location %>
    <%= f.input :comments %>
    <%= f.input :Approved %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
