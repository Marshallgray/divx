<table class="table">
  <thread>
    <tr>
      <th>Name</th>
      <th>Video</th>
      <th>Bio</th>
      <th>Resume</th>
      <th>Suburb</th>
      <th>Latitude</th>
      <th>Longitude</th>
        <th colspan="3"></th>
    </tr>
  </thread>

  <tbody>
    <% @talents.each do |talent| %>
      <tr>
        <td><%= talent.user.profile.full_name %></td>
        <td><%= talent.video %></td>
        <td><%= talent.bio %></td>
        <td><%= talent.resume %></td>
        <td><%= talent.suburb %></td>
        <td><%= talent.latitude %></td>
        <td><%= talent.longitude %></td>
          <td><%= link_to 'Show', talent, class: 'btn btn' %></td>
        <td><%= link_to 'Edit', edit_talent_path(talent), class: 'btn btn-edit' %></td>
        <td><%= link_to 'Delete', talent, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-destroy' %></td>
      </tr>
    <% end %>
  </tbody>
</table>
